{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug (Meson) (NASM input from ./builddir/input)",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/builddir/src/keccc",
      "args": [
        "--output",
        "./builddir/out.asm",
        "--target",
        "nasm",
        "./builddir/input"
      ],
      "type": "cppdbg",
      "request": "launch",
      "stopAtEntry": false,
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
